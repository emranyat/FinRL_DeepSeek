  for val in ${wrapper_extra_cxxflags}; do
                   if test "x$val" = "x$arg"; then :
  opal_append=0
fi
               done
fi
        if test "$opal_append" = "1"; then :
  if test -z "$wrapper_extra_cxxflags"; then :
  wrapper_extra_cxxflags=$arg
else
  wrapper_extra_cxxflags="$wrapper_extra_cxxflags $arg"
fi
fi
    done


