          { $as_echo "$as_me:${as_lineno-$LINENO}: No platform patches in ${patch_dir}" >&5
$as_echo "$as_me: No platform patches in ${patch_dir}" >&6;}
        fi
    else
        OPAL_DEFAULT_MCA_PARAM_CONF=openmpi-mca-params.conf

    fi


