# Some helper script functions.  Unfortunately, we cannot use  kinds
# of arguments here because of the m4 substitution.  So we have to set
# special variable names before invoking the function.  :-\

opal_show_title() {
  cat <<EOF

============================================================================
== ${1}
============================================================================
EOF

# 1 is the message
# 2 is whether to put a prefix or not
if test -n "1"; then
    echo "configure:5947: === ${1}" >&5
else
    echo === ${1} >&5
fi
}


opal_show_subtitle() {
  cat <<EOF

*** ${1}
EOF

# 1 is the message
# 2 is whether to put a prefix or not
if test -n "1"; then
    echo "configure:5963: *** ${1}" >&5
else
    echo *** ${1} >&5
fi
}


opal_show_subsubtitle() {
  cat <<EOF

+++ ${1}
EOF

# 1 is the message
# 2 is whether to put a prefix or not
if test -n "1"; then
    echo "configure:5979: +++ ${1}" >&5
else
    echo +++ ${1} >&5
fi
}

opal_show_subsubsubtitle() {
  cat <<EOF

--- ${1}
EOF

# 1 is the message
# 2 is whether to put a prefix or not
if test -n "1"; then
    echo "configure:5994: --- ${1}" >&5
else
    echo --- ${1} >&5
fi
}

opal_show_verbose() {
  if test "$V" = "1"; then
      cat <<EOF
+++ VERBOSE: ${1}
EOF

# 1 is the message
# 2 is whether to put a prefix or not
if test -n "1"; then
    echo "configure:6009: --- ${1}" >&5
else
    echo --- ${1} >&5
fi
  fi
}

